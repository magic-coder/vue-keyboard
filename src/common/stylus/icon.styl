@import "variable"

@font-face
  font-family 'aibaoTech'
  src url(icomoon-font-path + '/aibaoTech.ttf?ccdm6f') format('truetype'), url(icomoon-font-path + '/aibaoTech.woff?ccdm6f') format('woff'), url(icomoon-font-path + '/aibaoTech.svg?ccdm6f#aibaoTech') format('svg')
  font-weight normal
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'aibaoTech' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-delete
  &:before
    content icon-delete
